{
	"settings":{
		"number_of_shards": 10,
		"number_of_replicas": 0
	},
	"mappings":{
		"accesslog":{
			"_source": {
				"enabled": false
			  },
			"properties":{
				"Time":{
					"type":"date"
				},
				"Clientip":{
					"type":"ip"
				},
				"SEIP":{
					"type":"ip"
				},
				"HTTPStatus":{
					"type":"keyword"
				},
				"Urlhost":{
					"type":"keyword"
				}
			}
		}
	},
	"alias": {
		"errori" : {
            "filter" : {
                "range" : {"HTTPStatus": {"gte": 400} }
            }
	}
	} 
}